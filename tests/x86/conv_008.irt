--TEST--
008: type conversion FP2FP
--TARGET--
x86
--ARGS--
-S
--CODE--
{
	l_1 = START(l_4);
	float x = PARAM(l_1, "x", 1);
	double ret = FP2FP(x);
	l_4 = RETURN(l_1, ret);
}
--EXPECT--
test:
	subl $8, %esp
	movss 0xc(%esp), %xmm0
	cvtss2sd %xmm0, %xmm0
	movsd %xmm0, (%esp)
	fldl (%esp)
	addl $8, %esp
	retl
