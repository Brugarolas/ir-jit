--TEST--
002: COND(D,I,I)
--TARGET--
Windows-x86_64
--ARGS--
-S
--CODE--
{
	l_1 = START(ret);
	double x = PARAM(l_1, "x", 1);
	int32_t y = PARAM(l_1, "y", 2);
	int32_t z = PARAM(l_1, "z", 3);
	int32_t val = COND(x, y, z);
	ret = RETURN(l_1, val);
}
--EXPECT--
test:
	ucomisd .L4(%rip), %xmm0
	jp .L1
	je .L2
.L1:
	movl %edi, %eax
	jmp .L3
.L2:
	movl %esi, %eax
.L3:
	retq
.rodata
	.db 0x90, 0x90, 0x90, 0x90, 0x90, 0x90, 0x90, 0x90, 0x90, 0x90, 0x90, 0x90, 0x90
.L4:
	.db 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00
