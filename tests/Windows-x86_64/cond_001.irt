--TEST--
001: COND(I,I,I)
--TARGET--
Windows-x86_64
--ARGS--
-S
--CODE--
{
	l_1 = START(ret);
	int32_t x = PARAM(l_1, "x", 1);
	int32_t y = PARAM(l_1, "y", 2);
	int32_t z = PARAM(l_1, "z", 3);
	int32_t val = COND(x, y, z);
	ret = RETURN(l_1, val);
}
--EXPECT--
test:
	testl %ecx, %ecx
	je .L1
	movl %edx, %edx
	jmp .L2
.L1:
	movl %r8d, %r8d
.L2:
	retq
