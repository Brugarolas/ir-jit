--TEST--
003: COND(I,D,D)
--TARGET--
Windows-x86_64
--ARGS--
-S
--CODE--
{
	l_1 = START(ret);
	int32_t x = PARAM(l_1, "x", 1);
	double y = PARAM(l_1, "y", 2);
	double z = PARAM(l_1, "z", 3);
	double val = COND(x, y, z);
	ret = RETURN(l_1, val);
}
--EXPECT--
test:
	testq %rdi, %rdi
	je .L1
	jmp .L2
.L1:
	movapd %xmm1, %xmm0
.L2:
	retq
