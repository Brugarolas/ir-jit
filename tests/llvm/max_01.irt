--TEST--
001: max function
--ARGS--
--emit-llvm
--CODE--
{
	l_1 = START(l_4);
	int32_t x = PARAM(l_1, "x", 1);
	int32_t y = PARAM(l_1, "y", 2);
	int32_t ret = MAX(x, y);
	l_4 = RETURN(l_1, ret);
}
--EXPECT--
define i32 @test(i32 %d2, i32 %d3)
{
	%d4 = call i32 @llvm.smax.i32(i32 %d3, i32 %d2)
	ret i32 %d4
}
