--TEST--
003: type conversion BITS
--ARGS--
--emit-c
--CODE--
{
	l_1 = START(l_4);
	float x = PARAM(l_1, "x", 1);
	uint8_t ret = BITS(x);
	l_4 = RETURN(l_1, ret);
}
--EXPECT--
uint8_t test(float x)
{
	float d_1 = x;
	uint8_t d_2;
	{union {float f; uint32_t bits;} _u; _u.f = d_1; d_2 = _u.bits;}
	return d_2;
}
