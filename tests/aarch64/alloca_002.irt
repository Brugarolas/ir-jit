--TEST--
002: alloca function
--TARGET--
aarch64
--ARGS--
-S
--CODE--
{
	l_1 = START(l_4);
	int32_t x = PARAM(l_1, "x", 1);
	uintptr_t ret, l_2 = ALLOCA(l_1, x);
	l_4 = RETURN(l_2, ret);
}
--EXPECT--
test:
	stp x29, x30, [sp, #-0x10]!
	mov x29, sp
	add x0, x0, #7
	and x0, x0, #0xfffffffffffffff8
	sub sp, sp, x0
	mov x0, sp
	mov sp, x29
	ldp x29, x30, [sp], #0x10
	ret
