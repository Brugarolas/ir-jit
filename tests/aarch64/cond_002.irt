--TEST--
002: COND(D,I,I)
--TARGET--
aarch64
--ARGS--
-S
--CODE--
{
	l_1 = START(ret);
	double x = PARAM(l_1, "x", 1);
	int32_t y = PARAM(l_1, "y", 2);
	int32_t z = PARAM(l_1, "z", 3);
	int32_t val = COND(x, y, z);
	ret = RETURN(l_1, val);
}
--EXPECT--
test:
	fcmp d0, #0.0
	csel w0, w0, w1, eq
	ret
