--TEST--
002: mul_ov function
--TARGET--
aarch64
--ARGS--
-S
--CODE--
{
	uint32_t c = 2;
	l_1 = START(l_2);
	uint32_t x = PARAM(l_1, "x", 1);
	uint32_t ret = MUL_OV(x, c);
	bool ov = OVERFLOW(ret);
	l_2 = RETURN(l_1, ov);
}
--EXPECT--
test:
	movz w1, #0x2
	umull x0, w0, w1
	cmp xzr, x0, lsr #32
	cset w0, ne
	ret
