--TEST--
002: EQ(AND(_,_), 0) -> TEST
--TARGET--
x86_64
--ARGS--
-S
--CODE--
{
	int64_t c1 = 0;
	int64_t c2 = 0x300000000;
	l_1 = START(l_2);
	int64_t x = PARAM(l_1, "x", 1);
	int64_t d_1 = AND(x, c2);
	bool d_2 = EQ(d_1, c1);
	l_2 = RETURN(l_1, d_2);
}
--EXPECT--
test:
	movabsq $0x300000000, %rax
	testq %rax, %rdi
	sete %al
	retq
