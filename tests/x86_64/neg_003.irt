--TEST--
003: neg function
--TARGET--
x86_64
--ARGS--
-S
--CODE--
{
	l_1 = START(l_4);
	float x = PARAM(l_1, "x", 1);
	float ret = NEG(x);
	l_4 = RETURN(l_1, ret);
}
--EXPECT--
test:
	xorps .L1(%rip), %xmm0
	retq
.rodata
	.db 0x90, 0x90, 0x90, 0x90, 0x90, 0x90, 0x90, 0x90
.L1:
	.db 0x00, 0x00, 0x00, 0x80, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00
