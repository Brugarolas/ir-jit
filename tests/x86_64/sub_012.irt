--TEST--
012: sub function
--TARGET--
x86_64
--ARGS--
-S -mavx
--CODE--
{
	double c = 0.5;
	l_1 = START(l_4);
	double x = PARAM(l_1, "x", 1);
	double ret = SUB(c, x);
	l_4 = RETURN(l_1, ret);
}
--EXPECT--
test:
	vmovsd .L1(%rip), %xmm1
	vsubsd %xmm0, %xmm1, %xmm0
	retq
.rodata
	.db 0x90, 0x90, 0x90
.L1:
	.db 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0xe0, 0x3f
