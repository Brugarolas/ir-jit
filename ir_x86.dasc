||		ir_reg fp = (ctx->flags & IR_USE_FRAME_POINTER) ? IR_REG_RBP : IR_REG_RSP;
|		ASM_MEM_REG_OP op, type, [Ra(fp)+offset], src
||	} while (0);
|.endmacro

